
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option channel 'auto'
	option txpower '12'
	option country 'JP'

config wifi-iface
	option device 'radio0'
	option ssid 'iddqd,idkfa.local'
	option mode 'sta'
	option encryption 'psk2'
	option key 'yabudupovelitelemmira1961198519912012'
	option network 'lan'
	option 'client_bridge' '1'


