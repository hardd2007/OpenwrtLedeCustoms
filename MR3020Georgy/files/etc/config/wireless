
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option channel 'auto'
	option txpower '10'
	option country 'US'

config wifi-iface
	option device 'radio0'
	option ssid 'iddqd,idkfa.local'
	option mode 'sta'
	option encryption 'psk-mixed'
	option key 'yabudupovelitelemmira1961198519912012'
	option network 'lan wwan'

