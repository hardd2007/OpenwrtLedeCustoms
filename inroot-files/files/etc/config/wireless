
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/ar933x_wmac'
	option htmode 'HT20'
	option txpower '4'
	option country 'UZ'
	option channel '8'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option ssid 'home.local'
	option wmm '0'
	option encryption 'psk-mixed'
	option key 'radioroman'

