
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option 'type' 'bridge'
	option proto 'static'
	option ipaddr '192.168.4.254'
	option netmask '255.255.255.0'
	option delegate '0'
	option ifname 'eth0'

